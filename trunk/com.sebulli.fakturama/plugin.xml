      </editor>
      <editor
            class="com.sebulli.fakturama.editors.BrowserEditor"
            default="false"
            icon="icons/16/www_16.png"
            id="com.sebulli.fakturama.editors.browserEditor"
            name="Browser Editor">
      </editor>
      <editor
            class="com.sebulli.fakturama.editors.DummyEditor"
            default="false"
            id="com.sebulli.fakturama.editors.dummyEditor"
            name="Dummy Editor">
      </editor>
