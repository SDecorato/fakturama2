            class="com.sebulli.fakturama.exportsales.ExportSalesWizard"
            icon="icons/16/export_16.png"
            id="com.sebulli.fakturama.exportsales.exportSalesWizard"
            name="Zusammenfassung Umsätze">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="com.sebulli.fakturama.Startup"></startup>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.sebulli.fakturama.preferences.PreferenceInitializer">
      </initializer>
   </extension>

</plugin>
