            id="com.sebulli.fakturama.actions.selectWorkspace"
            name="Select Workspace">
      </command>
      <command
            description="Web Shop Import"
            id="com.sebulli.fakturama.actions.webShopImport"
            name="Web Shop Import">
      </command>
      <command
            description="Save"
            id="com.sebulli.fakturama.actions.save"
            name="Save">
      </command>
